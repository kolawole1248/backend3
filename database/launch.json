{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch CSE340 Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js",
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "restart": true,
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "run",
                "dev"
            ]
        }
    ]
}